<p id="notice"><%= notice %></p>

<h1>Cell Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Relative</th>
      <th>Relative</th>
      <th>Relative</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cell_numbers.each do |cell_number| %>
      <tr>
        <td><%= cell_number.relative %></td>
        <td><%= cell_number.relative %></td>
        <td><%= cell_number.relative %></td>
        <td><%= link_to 'Show', cell_number %></td>
        <td><%= link_to 'Edit', edit_cell_number_path(cell_number) %></td>
        <td><%= link_to 'Destroy', cell_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cell Number', new_cell_number_path %>
